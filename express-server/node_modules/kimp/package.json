{
    "name": "kimp",
    "version": "1.3.0",
    "description": "Lightweight ID generator",
    "sideEffects": false,
    "main": "dist/main/kimp.js",
    "module": "dist/module/kimp.js",
    "types": "types",
    "scripts": {
        "build": "npm run build:ts && npm run build:roll && npm run test",
        "build:ts": "tsc -p ./src/tsconfig.json",
        "build:roll": "rollup -c",
        "test": "jest"
    },
    "keywords": [
        "uuid",
        "ids",
        "id_generator",
        "keys",
        "crypto"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/herbievine/kimp.git"
    },
    "author": "Herbie Vine",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/herbievine/kimp/issues"
    },
    "homepage": "https://github.com/herbievine/kimp#readme",
    "devDependencies": {
        "@babel/core": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@types/jest": "^26.0.3",
        "babel-jest": "^26.1.0",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-es2015": "^6.24.1",
        "jest": "^26.1.0",
        "rollup": "^2.19.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^6.1.0"
    }
